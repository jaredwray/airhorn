{
  "name": "@airhornjs/twilio",
  "version": "5.1.0",
  "description": "Twilio SMS and SendGrid Email provider for Airhorn notification system",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "lint": "biome check --write --error-on-warnings",
    "test": "pnpm lint && vitest run --coverage",
    "test:ci": "biome check --error-on-warnings && vitest run --coverage",
    "clean": "rimraf ./dist ./coverage ./node_modules ./package-lock.json ./pnpm-lock.yaml",
    "prepublishOnly": "pnpm build",
    "build:publish": "pnpm publish --access public --no-git-checks",
    "build": "rimraf ./dist && tsup src/index.ts --format esm --dts --clean"
  },
  "keywords": [
    "airhorn",
    "twilio",
    "sendgrid",
    "sms",
    "email",
    "notifications",
    "messaging"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@sendgrid/mail": "^8.1.6",
    "twilio": "^5.10.4"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.4",
    "@types/node": "^24.10.0",
    "@vitest/coverage-v8": "^4.0.7",
    "rimraf": "^6.1.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.7"
  },
  "peerDependencies": {
    "airhorn": "workspace:^"
  },
  "files": [
    "dist",
    "LICENSE"
  ]
}