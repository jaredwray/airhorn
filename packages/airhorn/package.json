{
  "name": "airhorn",
  "version": "5.0.1",
  "description": "Cloud Notification Library",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "engines": {
    "node": ">=20"
  },
  "repository": "https://github.com/jaredwray/airhorn.git",
  "author": "Jared Wray <me@jaredwray.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "lint": "biome check --write --error-on-warnings",
    "test": "pnpm lint && vitest run --coverage",
    "test:ci": "biome check --error-on-warnings && vitest run --coverage",
    "clean": "rimraf ./dist ./coverage ./node_modules ./package-lock.json ./pnpm-lock.yaml",
    "prepare": "pnpm build",
    "build:publish": "pnpm build && pnpm publish --access public",
    "build": "rimraf ./dist && tsup src/index.ts --format esm --dts --clean"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.2",
    "@types/node": "^24.3.0",
    "@vitest/coverage-v8": "^3.2.4",
    "docula": "^0.20.0",
    "rimraf": "^6.0.1",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "cacheable": "^1.10.4",
    "ecto": "^4.4.1",
    "hookified": "^1.12.0",
    "writr": "^4.4.6"
  },
  "files": [
    "dist",
    "LICENSE"
  ]
}